<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.thomaskuenneth.tkmoodley" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/smiley_gut"
        android:label="@string/app_name" >
        <activity
            android:name="com.thomaskuenneth.tkmoodley.TKMoodley"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.thomaskuenneth.tkmoodley.History" />

        <provider
            android:name="com.thomaskuenneth.tkmoodley.TKMoodleyProvider"
            android:authorities="com.thomaskuenneth.tkmoodley.tkmoodleyprovider"
            android:exported="true" />
    </application>

</manifest>